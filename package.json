{
  "name": "music-electron-client",
  "version": "0.0.1",
  "description": "An Electron application with Vue and TypeScript",
  "main": "./out/main/index.js",
  "author": "luoyue",
  "homepage": "https://doc.vkeys.cn",
  "license": "GPL-3.0",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint --cache .",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "vue-tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "start": "electron-vite preview",
    "dev": "electron-vite dev",
    "build": "npm run typecheck && electron-vite build",
    "postinstall": "electron-builder install-app-deps",
    "build:unpack": "npm run build && electron-builder --dir",
    "build:win": "npm run build && electron-builder --win",
    "build:mac": "npm run build && electron-builder --mac",
    "build:linux": "npm run build && electron-builder --linux"
  },
  "dependencies": {
    "@electron-toolkit/preload": "^3.0.1",
    "@electron-toolkit/utils": "^4.0.0",
    "electron-updater": "^6.6.2",
    "sqlite3": "^5.1.7"
  },
  "devDependencies": {
    "@applemusic-like-lyrics/lyric": "^0.2.4",
    "@applemusic-like-lyrics/vue": "^0.1.5",
    "@electron-toolkit/eslint-config-prettier": "3.0.0",
    "@electron-toolkit/eslint-config-ts": "^3.0.0",
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@iconify-json/qlementine-icons": "^1.2.5",
    "@pixi/app": "^7.4.3",
    "@pixi/core": "^7.4.3",
    "@pixi/display": "^7.4.3",
    "@pixi/filter-blur": "^7.4.3",
    "@pixi/filter-bulge-pinch": "^5.1.1",
    "@pixi/filter-color-matrix": "^7.4.3",
    "@pixi/filter-noise": "^7.4.3",
    "@pixi/sprite": "^7.4.3",
    "@types/howler": "^2.2.12",
    "@types/node": "^22.14.0",
    "@unocss/postcss": "^66.1.0-beta.8",
    "@unocss/preset-icons": "^66.1.0-beta.8",
    "@vitejs/plugin-vue": "^5.2.3",
    "@vitejs/plugin-vue-jsx": "^4.1.2",
    "@vueuse/components": "^13.0.0",
    "@vueuse/core": "^13.0.0",
    "@vueuse/electron": "^13.0.0",
    "axios": "^1.8.4",
    "electron": "^35.1.3",
    "electron-builder": "^26.0.12",
    "electron-vite": "^3.1.0",
    "element-plus": "^2.9.7",
    "eslint": "^9.23.0",
    "eslint-plugin-vue": "^10.0.0",
    "howler": "^2.2.4",
    "jss": "^10.10.0",
    "jss-preset-default": "^10.10.0",
    "pinia": "^3.0.1",
    "pinia-plugin-persistedstate": "^4.2.0",
    "prettier": "^3.5.3",
    "sass-embedded": "^1.86.3",
    "typescript": "^5.8.2",
    "unocss": "^66.1.0-beta.8",
    "unplugin-auto-import": "^19.1.2",
    "unplugin-vue-components": "^28.4.1",
    "vite": "^6.2.5",
    "vite-plugin-vue-devtools": "^7.7.2",
    "vite-plugin-wasm": "^3.4.1",
    "vue": "^3.5.13",
    "vue-eslint-parser": "^10.1.2",
    "vue-router": "^4.5.0",
    "vue-tsc": "^2.2.8"
  }
}
